	"dependencies": {
		"@types/express": "^4.17.13",
		"@types/node": "^17.0.21",
		"dotenv": "^16.0.0",
		"express": "^4.17.3",
		"mongoose": "^6.2.6",
		"nodemon": "^2.0.15",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.2"
	}
}
